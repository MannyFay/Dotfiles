# Laravel Sail - Sail command (./vendor/bin/sail)
sail

# Laravel Sail - Start Sail in detached mode (./vendor/bin/sail up -d)
sud

# Laravel Sail - Start Sail (./vendor/bin/sail up)
su

# Laravel Sail - Stop Sail (./vendor/bin/sail down)
sd

# Laravel Sail - Run tests (.vendor/bin/sail test)
st





# Laravel Sail: Show artisan commands
salc

# Laravel Sail: Start Composer update
scu

# Laravel Sail: Publish vendor directory
svp
